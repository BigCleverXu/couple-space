<t-cell title="{{title}}" hover note="{{dateText || ''}}" arrow bindtap="showPicker" />
<!-- 年月日 -->
<t-date-time-picker title="选择日期" visible="{{visible}}" mode="date" defaultValue="{{date}}" format="YYYY-MM-DD" bindchange="onConfirm" bindpick="onColumnChange" bindcancel="hidePicker" start="{{start}}" end="{{end}}" />